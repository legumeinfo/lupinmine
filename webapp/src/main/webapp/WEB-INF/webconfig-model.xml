    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="secondaryIdentifier" label="ID"/>
      <fieldconfig fieldExpr="symbol" label="Symbol"/>
      <fieldconfig fieldExpr="type" label="Type"/>
