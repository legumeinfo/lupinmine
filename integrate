#!/bin/sh

rm -f intermine.log

echo ""
echo "##########################################################################################################################################"
echo "##########################################################################################################################################"
echo "########################################################## INTEGRATE #####################################################################"
echo "##########################################################################################################################################"
echo "##########################################################################################################################################"
echo ""

## ontologies and InterPro
./gradlew integrate -Psource=go --stacktrace
./gradlew integrate -Psource=so --stacktrace
./gradlew integrate -Psource=plant-ontology --stacktrace
./gradlew integrate -Psource=plant-trait-ontology --stacktrace
./gradlew integrate -Psource=interpro --stacktrace
./gradlew integrate -Psource=interpro-go --stacktrace

## LIS gene families
./gradlew integrate -Psource=lis.fam1-genefamily --stacktrace

###########
## lupal ##
###########

./gradlew integrate -Psource=lupal-about

## lupal.Amiga
./gradlew integrate -Psource=lupal.Amiga.gnm1-chromosome-fasta
./gradlew integrate -Psource=lupal.Amiga.gnm1.ann0-gff
./gradlew integrate -Psource=lupal.Amiga.gnm1.ann0-cds-fasta
./gradlew integrate -Psource=lupal.Amiga.gnm1.ann0-mrna-fasta
./gradlew integrate -Psource=lupal.Amiga.gnm1.ann0-protein-fasta

###########
## lupan ##
###########

./gradlew integrate -Psource=lupan-about

## lupan.Tanjil
./gradlew integrate -Psource=lupan.Tanjil.gnm1-chromosome-fasta
./gradlew integrate -Psource=lupan.Tanjil.gnm1.ann1-gff
./gradlew integrate -Psource=lupan.Tanjil.gnm1.ann1-cds-fasta
./gradlew integrate -Psource=lupan.Tanjil.gnm1.ann1-protein-fasta
./gradlew integrate -Psource=lupan.Tanjil.gnm1.ann1.syn1-synteny

 
